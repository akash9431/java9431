import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      int a=4;
        int b=8;
        int c=5;
        Scanner s=new Scanner(System.in);
        a=s.nextInt();
        b=s.nextInt();
        c=s.nextInt();
        if(a>b){
            System.out.println(a);
        }
            else if(a<c){
                System.out.println(b);
    }
        else{
            System.out.println(c);
        }
    }
}
